# PUBLIC frontend environment variables
# Set the backend base URL if the frontend and backend run on different origins.
# Example for local dev with backend on 3001:
# NEXT_PUBLIC_BACKEND_URL=http://localhost:3001

# Set the canonical frontend base URL if needed for links:
# NEXT_PUBLIC_FRONTEND_BASE_URL=http://localhost:3000

# Optional: force dev server port
# PORT=3000

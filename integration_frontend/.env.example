# Copy to .env.local for local dev, or set as container env variables in deployment.

# Backend absolute URL (recommended in preview environments)
# If not set, the frontend will call the backend via same-origin relative URLs.
NEXT_PUBLIC_BACKEND_URL=https://vscode-internal-22588-beta.beta01.cloud.kavia.ai:3001

# Optional public base URL of the frontend (used by some deployments)
# NEXT_PUBLIC_FRONTEND_BASE_URL=https://your-frontend.example.com

# Atlassian/Jira OAuth related envs (not consumed directly by the frontend runtime here,
# but documented for completeness in the container environment)
# NEXT_PUBLIC_JIRA_OAUTH_CLIENT_ID=
# NEXT_PUBLIC_JIRA_OAUTH_CLIENT_SECRET=
# NEXT_PUBLIC_JIRA_OAUTH_REDIRECT_URI=
# NEXT_PUBLIC_JIRA_CLIENT_ID=
# NEXT_PUBLIC_ATLASSIAN_CLIENT_ID=
# NEXT_PUBLIC_ATLASSIAN_CLIENT_SECRET=
# NEXT_PUBLIC_ATLASSIAN_REDIRECT_URI=
# NEXT_PUBLIC_ATLASSIAN_SCOPES=
# NEXT_PUBLIC_NEXT_PUBLIC_BACKEND_URL=
# NEXT_PUBLIC_NEXT_PUBLIC_APP_BASE_URL=
# NEXT_PUBLIC_BACKEND_PUBLIC_BASE_URL=
# NEXT_PUBLIC_BACKEND_CORS_ORIGINS=
